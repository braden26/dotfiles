" Better Clipboard
set clipboard=unnamedplus

" Syntax Highlighting
filetype plugin on
syntax on

" Powerline
let g:powerline_pycmd="py3"
set laststatus=2

" Tabs and Spacing
set tabstop=4
set softtabstop=4

" Show Line number
set number

" Wild menu
set wildmenu

" Redraw only when need to, makes macros faster apparently

